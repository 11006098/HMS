<Window x:Class="HMS.View.Symptoms"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Symptoms" Height="300" Width="600">
    <Canvas>
        <Label Name="lblSymtomps" Height="25" Width="95" Canvas.Left="0" Canvas.Top="10" Content="Specialization"></Label>
        <ComboBox Name="txtBoxSymtomps" Height="25" Width="218" Canvas.Left="139" Canvas.Top="10"></ComboBox>
        <DatePicker Name="Appointment" Height="25" Width="105" Canvas.Left="400" Canvas.Top="10"></DatePicker>
        <Canvas Height="200" Width="560" Canvas.Top="50" Canvas.Left="10">
            <ListView Height="200" Width="560">
                <ListView.View>
                    <GridView>
                        <GridViewColumn Header="Name" Width="140"></GridViewColumn>
                         <GridViewColumn Header="Contact" Width="140"></GridViewColumn>
                        <GridViewColumn Header="Email ID" Width="140"></GridViewColumn>
                        <GridViewColumn Header="Select" Width="140"></GridViewColumn>
                    </GridView>
                </ListView.View>
            </ListView>
        </Canvas>
    </Canvas>
</Window>
